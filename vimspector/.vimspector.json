{
	"configurations": {
		"cargo - run": {
			"adapter": "vscode-cpptools",
			"configuration": {
				"name": "LLDB: Launch",
				"type": "cppdbg",
				"request": "launch",
				"cwd": "${workspaceRoot}",
				"args": [],
				"program": "${workspaceRoot}/target/debug/${TestIdentifier}",
				"stopAtEntry": true,
				"externalConsole": true
			}
		},
		"cargo - test": {
			"adapter": "vscode-cpptools",
			"configuration": {
				"name": "LLDB: Launch",
				"type": "cppdbg",
				"request": "launch",
				"cwd": "${workspaceRoot}",
				"args": [],
				"program": "${workspaceRoot}/target/debug/deps/${TestIdentifier}",
				"stopAtEntry": true,
				"externalConsole": true
			}
		},
		"go - run": {
			"adapter": "vscode-go",
			"configuration": {
				"request": "launch",
				"program": "${fileDirname}",
				"mode": "debug",
				"dlvToolPath": "$GOPATH/bin/dlv"
			}
		}
	}
}
